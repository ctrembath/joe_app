<div class="row" id="gig-header">
	<div class="small-12 columns">
		<h2>Playing Out</h2>
	</div>
</div>

<div class="row" id="gig-listings">
	<div class="small-12 columns">
		<ul>
			<% @gigs.each do |gig| %>
			<li id="gig-night">
				<%= gig.date %><br>
				<a id="venue" href="<%= gig.url %>"><%= gig.venue %></a>
			</li>
			<% end %>
		</ul>
	</div>
</div>
	