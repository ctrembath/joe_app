<div id="homepage">
	<h1 id="title">Joe Hertz</h1>
	<div class="row">
	  <div class="small-12 columns">
			<div id="logo">
			</div>
		</div>
	</div>

	<div class="row" id="gig-link">
		<div class="small-12 columns">
			<a href="/gigs">Next playing</a>
		</div>
	</div>

	<div class="row">
	  <div class="small-12 columns">
			<% @my_bucket.objects.each do |object| %>
				<li><%= object.key %></li>
			<% end %>
		</div>
	</div>

	<div class="row" id="track-one">
	  <div class="small-12 columns">
		  <div id="play-one"></div>
	 	</div>
	</div>
	<div class="row" id="track-two">
	  <div class="small-12 columns">
		  <div class="play"></div>
	 	</div>
	</div>
	<div class="row" id="track-three">
	  <div class="small-12 columns">
		  <div class="play"></div>
	 	</div>
	</div>
</div>
